        <attr name="textButton" format="string" />
        <attr name="paddingHorizontalButton" format="dimension" />
        <attr name="textAppearanceButton" format="reference" />
    </declare-styleable>

    <declare-styleable name="ExpansionLayout">
        <attr name="expansionExpanded" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpansionHeader">
        <attr name="expansionLayoutHeader" format="reference" />
